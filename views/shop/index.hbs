<div class="row">
  
  {{#each products}}
  <div class="col-lg-4 col-md-6 mb-4">
    <div class="card h-100">
      <a href="#">
        <img class="card-img-top" src="{{this.image}}" alt="">
      </a>
      <div class="card-body">
        <h4 class="card-title">
          <a href="#">{{this.name}}</a>
        </h4>
        <h5>in stock {{this.instock}}</h5><i class="fas fa-cart-plus"></i>

        <p class="card-text">{{this.store}}</p>
      </div>
      <div class="card-footer">
        <small class="text-muted">&#9733; &#9733; &#9733; &#9733; &#9734;</small>
      </div>
    </div>
  </div>
  {{/each}}

</div>